<?xml version="1.0" encoding="UTF-8"?>
<launch4jConfig>
  <dontWrapJar>false</dontWrapJar>
  <headerType>gui</headerType>
  <jar>dorothy.jar</jar>
  <outfile>dorothy.exe</outfile>
  <errTitle>Dorothy Error</errTitle>
  <chdir>.</chdir>
  <priority>normal</priority>
  <downloadUrl>https://adoptium.net/</downloadUrl>
  <supportUrl></supportUrl>
  <stayAlive>false</stayAlive>
  <restartOnCrash>false</restartOnCrash>
  <manifest>app.manifest</manifest>
  <icon>icon.ico</icon>
  <jre>
    <path>jre</path>
    <bundledJre64Bit>true</bundledJre64Bit>
    <bundledJreAsFallback>true</bundledJreAsFallback>
    <minVersion>17</minVersion>
    <maxVersion></maxVersion>
    <jdkPreference>preferJre</jdkPreference>
    <runtimeBits>64</runtimeBits>
    <initialHeapSize>256</initialHeapSize>
    <maxHeapSize>1024</maxHeapSize>
    <opt>-Djava.library.path=./native</opt>
    <opt>--module-path=lib;lib/platform-specific</opt>
    <opt>--add-modules=javafx.controls,javafx.fxml,javafx.graphics</opt>
  </jre>
</launch4jConfig> 